# Ejemplo de variables para staging
# Copiar a .env.staging y completar con valores reales
# NUNCA commitear .env.staging al repositorio

_DATABASE_URL=postgresql://user:password@host:5432/database
_AUTH_SECRET=your-secret-key-minimum-32-characters-long

# WOMPI Payment Gateway Configuration (Sandbox/Test)
WOMPI_ENV=test
WOMPI_PUBLIC_KEY=pub_test_xxxxxxxxxxxxxxxx
WOMPI_PRIVATE_KEY=prv_test_xxxxxxxxxxxxxxxx
WOMPI_INTEGRITY_SECRET=test_integrity_xxxxxxxxx
WOMPI_EVENTS_SECRET=test_events_xxxxxxxxx
NEXT_PUBLIC_WOMPI_CHECKOUT_URL=https://checkout.wompi.co/l/
NEXT_PUBLIC_APP_URL=https://your-staging-url.run.app
